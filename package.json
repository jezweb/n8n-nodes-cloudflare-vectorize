{
  "name": "n8n-nodes-cloudflare-vectorize",
  "version": "0.1.0",
  "description": "n8n community node for Cloudflare Vectorize vector database integration, featuring vector operations (insert/upsert/query), index management, metadata filtering, namespace support, and full AI Agent compatibility for semantic search and RAG implementations",
  "keywords": [
    "n8n-community-node-package",
    "cloudflare",
    "vectorize",
    "vector-database",
    "embeddings",
    "semantic-search",
    "ai-tools",
    "rag",
    "machine-learning",
    "similarity-search"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jezweb/n8n-nodes-cloudflare-vectorize#readme",
  "author": {
    "name": "Jez (Jeremy Dawes)",
    "email": "jeremy@jezweb.net",
    "url": "https://www.jezweb.com.au"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jezweb/n8n-nodes-cloudflare-vectorize.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"**/*.{js,ts}\" --ignore-path .gitignore",
    "lintfix": "eslint \"**/*.{js,ts}\" --ignore-path .gitignore --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CloudflareVectorizeApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CloudflareVectorize/CloudflareVectorize.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~8.32.0",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}